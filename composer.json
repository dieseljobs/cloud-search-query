{
    "name": "thelhc/cloud-search-query",
    "description": "An ORM-like wrapper for building AWS CloudSearch structured queries",
    "authors": [
        {
            "name": "Aaron Michael Music",
            "email": "aaronmichaelmusic@gmail.com"
        }
    ],
    "require": {
      "php": "~5.6|~7.0|~7.1|~7.2|~7.3",
      "illuminate/database": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.*",
      "illuminate/support": "5.1.* || 5.2.* || 5.3.* || 5.4.* || 5.5.* || 5.6.* || 5.7.* || 5.8.*",
      "aws/aws-sdk-php": "^3.19"
    },
    "require-dev": {
      "phpunit/phpunit": "^5.4 || ~6.0",
      "orchestra/testbench": "~3.0",
      "vlucas/phpdotenv": "^2.4"
    },
    "autoload": {
        "psr-4": {
            "TheLHC\\CloudSearchQuery\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "TheLHC\\CloudSearchQuery\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
